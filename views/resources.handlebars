  <!-- MY RESOURCES ================================================================================================== -->
  <div class="container">
    <div class="row">
      <div class="col-md-8">
        <h2>My Resources</h2>
        <form class="login">
          <div class="form-group">
            <h4>Add Resource</h4>
            <div>
              <label for="description">Topic:</label>
              <input id="topic" type="text" class="form-control" placeholder="Enter topic">
            </div>
            <br>
            <div>
              <label for="link">Link:</label>
              <input id="link" type="url" class="form-control" placeholder="http://">
            </div>
            <br>
            <div>
              <label for="description">Description:</label>
              <textarea id="description" class="form-control" rows="6"></textarea>
            </div>
            <br>
            <a class="btn btn-default" id="save" href="#" role="button" style="margin-right: 10px">SAVE</a>
            <a class="btn btn-default" id="cancel" href="#" role="button" style="margin-right: 20px">CANCEL</a>
            <div class="privacy" style="float: right">
              <label>
                <input type="checkbox" id="public"> Public
              </label>
            </div>
          </div>
        </form>
        <br/>
      </div>
    </div>
  </div>
  <!-- TOPICS ================================================================================================== -->
  <div class="row">
    <div class="col-md-12">
      <h4>My Resources</h4>
      <div class="table-responsive">
        <table id="mytable" class="table table-bordered" style="width:100%">
          <thead>
            <th>
              <input type="checkbox" id="checkall" />
            </th>
            <th>Topic</th>
            <th>Link</th>
            <th>Description</th>
            <th>Edit</th>
            <th>Delete</th>
          </thead>
          <tbody>
            {{#each resources}} 
              {{{<tr>}}} 
                  {{> resources/resource-block}} 
              {{{<tr>}}}  
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
